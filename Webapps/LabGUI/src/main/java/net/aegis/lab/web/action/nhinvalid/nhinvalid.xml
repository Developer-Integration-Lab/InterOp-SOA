    <package name="nhinvalid" namespace="/nhinvalid" extends="tiles-default">

        <action name="NhinvalidDashboard" class="net.aegis.lab.web.action.nhinvalid.NhinvalidDashboard">
            <result name="input" type="tiles">nhinvalid.dashboard</result>
            <result name="success" type="tiles">nhinvalid.dashboard</result>
            <result name="error" type="tiles">error.default</result>
