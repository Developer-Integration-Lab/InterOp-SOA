            <result name="success" type="tiles">assign.submission</result>
            <result name="error" type="tiles">error.default</result>
        </action>

        <action name="TestCaseHelp" class="net.aegis.lab.web.action.nhinvalid.TestCaseHelp">
            <result name="success">/WEB-INF/nhinvalid/TestCaseHelp.jsp</result>
            <result name="error" type="tiles">error.default</result>
