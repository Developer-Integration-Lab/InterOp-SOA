<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.predic8.plugin.membrane.perspectives.MembranePerspective"
            icon="icons/logo32.png"
            id="com.predic8.plugin.membrane.perspectives.MembranePerspective"
            name="Membrane Monitor">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.predic8.plugin.membrane.views.MembraneRulesViewCategory"
            name="membrane rules">
      </category>
      <category
            id="com.predic8.plugin.membrane.views.MembraneExchangesViewCategory"
            name="membrane exchanges">
      </category>
      <view
            category="com.predic8.plugin.membrane.views.MembraneRulesViewCategory"
            class="com.predic8.plugin.membrane.views.RuleDetailsView"
            icon="icons/rule_details.png"
            id="com.predic8.plugin.membrane.views.RuleDetailsView"
            name="Rule Details"
            restorable="true">
      </view>
      <view
            category="com.predic8.plugin.membrane.views.MembraneRulesViewCategory"
            class="com.predic8.plugin.membrane.views.RuleTableView"
            icon="icons/table.png"
            id="com.predic8.plugin.membrane.views.RuleTableView"
            name="Rule Table"
            restorable="true">
      </view>
      <view
            category="com.predic8.plugin.membrane.views.MembraneRulesViewCategory"
            class="com.predic8.plugin.membrane.views.RuleStatisticsView"
            icon="icons/calculator.png"
            id="com.predic8.plugin.membrane.views.RuleStatisticsView"
            name="Rule Statistics"
            restorable="true">
      </view>
      <view
            class="com.predic8.plugin.membrane.views.BrowserView"
            icon="icons/world_go.png"
            id="com.predic8.plugin.membrane.views.BrowserView"
            name="BrowserView"
            restorable="true">
      </view>
      <view
            class="com.predic8.plugin.membrane.views.ErrorLogView"
            id="com.predic8.plugin.membrane.views.ErrorLogView"
            name="Error Log"
            restorable="true">
      </view>
      <view
            category="com.predic8.plugin.membrane.views.MembraneExchangesViewCategory"
            class="com.predic8.plugin.membrane.views.ExchangesView"
            icon="icons/cascade.png"
            id="com.predic8.plugin.membrane.views.ExchangesView"
            name="Exchanges"
            restorable="true">
      </view>
      <view
            category="com.predic8.plugin.membrane.views.MembraneExchangesViewCategory"
            class="com.predic8.plugin.membrane.views.RequestView"
            icon="icons/message_out.png"
            id="com.predic8.plugin.membrane.views.RequestView"
            name="Request"
            restorable="true">
      </view>
      <view
            category="com.predic8.plugin.membrane.views.MembraneExchangesViewCategory"
            class="com.predic8.plugin.membrane.views.ResponseView"
            icon="icons/message_in.png"
            id="com.predic8.plugin.membrane.views.ResponseView"
            name="Response"
            restorable="true">
      </view>
      <view
            category="com.predic8.plugin.membrane.views.MembraneRulesViewCategory"
            class="com.predic8.plugin.membrane.views.RulesView"
            icon="icons/report_key.png"
            id="com.predic8.plugin.membrane.views.RulesView"
            name="Rules"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="com.predic8.plugin.membrane.preferences.MembraneMonitorPreferencePage"
            class="com.predic8.plugin.membrane.preferences.MessagePreferencePage"
            id="com.predic8.plugin.membrane.preferences.MessagePreferencePage"
            name="Message">
      </page>
      <page
            class="com.predic8.plugin.membrane.preferences.MembraneMonitorPreferencePage"
            id="com.predic8.plugin.membrane.preferences.MembraneMonitorPreferencePage"
            name="Membrane Monitor">
      </page>
      <page
            category="com.predic8.plugin.membrane.preferences.MembraneMonitorPreferencePage"
            class="com.predic8.plugin.membrane.preferences.ProxyPreferencePage"
            id="com.predic8.plugin.membrane.preferences.ProxyPreferencePage"
            name="Proxy">
      </page>
      <page
            category="com.predic8.plugin.membrane.preferences.MembraneMonitorPreferencePage"
            class="com.predic8.plugin.membrane.preferences.ExchangePreferencePage"
            id="com.predic8.plugin.membrane.preferences.ExchangePreferencePage"
            name="Exchange">
      </page>
      <page
            category="com.predic8.plugin.membrane.preferences.MembraneMonitorPreferencePage"
            class="com.predic8.plugin.membrane.preferences.SecurityPreferencePage"
            id="com.predic8.plugin.membrane.preferences.SecurityPreferencePage"
            name="Security SSL/TLS">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.helpSupport">
      <config
            class="com.predic8.plugin.membrane.help.MembraneHelpUI">
      </config>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml"
            plugin="com.predic8.plugin.membrane">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="testToc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="file"
               label="File">
            <command
                  commandId="loadConfiguration"
                  icon="icons/folder_add.png"
                  label="Load Configuration"
                  style="push">
            </command>
            <command
                  commandId="saveConfiguration"
                  icon="icons/disk.png"
                  label="Save Configuration"
                  style="push">
            </command>
            <separator
                  name="com.predic8.plugin.membrane.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="exit"
                  label="Exit"
                  style="push">
            </command>
         </menu>
         <menu
               label="Rules">
            <command
                  commandId="addRule"
                  icon="icons/add_rule.png"
                  label="&amp;Add Rule"
                  style="push">
            </command>
         </menu>
         <menu
               id="window"
               label="Window">
            <separator
                  name="com.predic8.plugin.membrane.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="showRuleStatistics"
                  icon="icons/calculator.png"
                  label="Show Rule Statistics"
                  style="push">
            </command>
            <command
                  commandId="showExchanges"
                  icon="icons/cascade.png"
                  label="Show Exchanges"
                  style="push">
            </command>
            <command
                  commandId="showRules"
                  icon="icons/report_key.png"
                  label="Show Rules"
                  style="push">
            </command>
            <command
                  commandId="showRequestView"
                  icon="icons/message_out.png"
                  label="Show Request View"
                  style="push">
            </command>
            <command
                  commandId="showResponseView"
                  icon="icons/message_in.png"
                  label="Show Response View"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="com.predic8.plugin.membrane.toolbar">
            <command
                  commandId="addRule"
                  icon="icons/add_rule.png"
                  style="push">
            </command>
            <separator
                  name="com.predic8.plugin.membrane.separator3"
                  visible="true">
            </separator>
            <command
                  commandId="saveConfiguration"
                  icon="icons/disk.png"
                  style="push">
            </command>
            <command
                  commandId="loadConfiguration"
                  icon="icons/folder_add.png"
                  style="push">
            </command>
            <separator
                  name="com.predic8.plugin.membrane.separator4"
                  visible="true">
            </separator>
            <command
                  commandId="showRules"
                  icon="icons/report_key.png"
                  style="push">
            </command>
            <command
                  commandId="showRuleStatistics"
                  icon="icons/calculator.png"
                  style="push">
            </command>
            <command
                  commandId="showExchanges"
                  icon="icons/cascade.png"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.LoadConfigurationCommand"
            id="loadConfiguration"
            name="Load Configuration">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.SaveConfigurationCommand"
            id="saveConfiguration"
            name="Save Configuration">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.AddRuleCommand"
            id="addRule"
            name="&amp;Add Rule">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.ShowRuleStatisticsCommand"
            id="showRuleStatistics"
            name="Show Rule Statistics">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.ShowExchangesCommand"
            id="showExchanges"
            name="Show Exchanges">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.ShowRulesCommand"
            id="showRules"
            name="Show Rules">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.ExitCommand"
            id="exit"
            name="Exit">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.ShowRequestViewCommand"
            id="showRequestView"
            name="Show Request View">
      </command>
      <command
            defaultHandler="com.predic8.plugin.membrane.commands.ShowResponseViewCommand"
            id="showResponseView"
            name="Show Response View">
      </command>
   </extension>

</plugin>
